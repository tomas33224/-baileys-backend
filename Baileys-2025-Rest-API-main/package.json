{
  "name": "baileys-api",
  "version": "1.0.0",
  "description": "REST API wrapper for Baileys WhatsApp Web library with management dashboard",
  "keywords": [
    "whatsapp",
    "automation",
    "rest-api",
    "baileys",
    "api-wrapper"
  ],
  "homepage": "https://github.com/WhiskeySockets/Baileys/",
  "repository": {
    "url": "git@github.com:WhiskeySockets/Baileys.git"
  },
  "license": "MIT",
  "author": "Rajeh Taher",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "files": [
    "dist/*",
    "lib/*",
    "WAProto/*.ts",
    "WAProto/*.js",
    "WASignalGroup/*.js",
    "engine-requirements.js"
  ],
  "scripts": {
    "build": "tsc --skipLibCheck --noEmitOnError",
    "build:all": "tsc --skipLibCheck --noEmitOnError && typedoc",
    "build:docs": "typedoc",
    "build:tsc": "tsc --skipLibCheck --noEmitOnError",
    "start": "node dist/app.js",
    "start:api": "PORT=3001 node dist/app.js",
    "start:minimal": "PORT=3001 node src/app-minimal.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/app.ts",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "node --inspect -r ts-node/register Example/example.ts",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "prepack": "tsc",
    "prepare": "tsc",
    "preinstall": "node ./engine-requirements.js",
    "release": "release-it",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^9.1.3",
    "@prisma/client": "^5.7.1",
    "@whiskeysockets/eslint-config": "github:whiskeysockets/eslint-config",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "libsignal": "github:pst4r8/libsignal-node",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "music-metadata": "^7.12.3",
    "pino": "^9.6",
    "protobufjs": "^7.2.4",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.0.0",
    "jest": "^27.0.6",
    "jimp": "^0.16.1",
    "json": "^11.0.0",
    "link-preview-js": "^3.0.0",
    "open": "^8.4.2",
    "prisma": "^5.7.1",
    "protobufjs-cli": "^1.1.3",
    "release-it": "^15.10.3",
    "sharp": "^0.32.6",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "audio-decode": "^2.1.3",
    "jimp": "^0.16.1",
    "link-preview-js": "^3.0.0",
    "sharp": "^0.32.6"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0"
  }
}
